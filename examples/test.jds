// To Multiply: REGA: 3, REGX: 3

@MEMORY_BEGIN: #9000h

@INFO JDASM version *__VERSION__*

`MUL_LOOP`
ADD #100+yh
DEX
CPX $00h
JNZ %MUL_LOOP%
RET

`MUL`
TSX
TXA
TAY
INY
INY
INY
LDA #100+yh
JSR %MUL_LOOP%
RET

@MEMORY_BEGIN: #8000h
LDA 'A'
LDX $00h
INT $01h

LDA $03h
PUSHA
LDX $03h
@INFO MUL = `MUL`
JSR %MUL%
PUSHA

EXIT